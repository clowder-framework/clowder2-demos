{
  "@context": "https://vega.github.io/vega/examples/word-cloud/",
  "name": "ncsa.word-cloud-extractor",
  "version": "1.0",
  "description": "Word cloud extractor using VegaLite. Visualize word cloud out of a csv file with 2 columns.",
  "author": "Dipannita Dey <ddey2@illinois.edu>",
  "contributors": [],
  "contexts": [
    {
      "url" : "https://vega.github.io/vega/examples/word-cloud/"
    }
  ],
  "repository": [
    {
      "repType": "git",
      "repUrl": "https://github.com/clowder-framework/clowder2-demos.git"
    }
  ],
  "process": {
    "file": [
      "csv"
    ]
  },
  "external_services": [],
  "dependencies": [],
  "bibtex": [],
  "parameters": {
    "schema": {
      "height": {
        "type": "integer",
        "title": "Height of visualization card in px",
        "default": 400
      },
      "width": {
        "type": "integer",
        "title": "Width of visualization card in px",
        "default": 350
      }
    },
    "form": [
      {
        "key": "height",
        "type": "input"
      },
      {
        "key": "width",
        "type": "input"
      }
    ]
  }
}
